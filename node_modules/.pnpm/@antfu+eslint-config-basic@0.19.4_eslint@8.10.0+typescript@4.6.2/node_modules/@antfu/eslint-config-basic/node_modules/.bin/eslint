else
  export NODE_PATH="$NODE_PATH:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/eslint@8.10.0/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/node_modules:/Users/gaomy/Documents/学习/vue3学习/node_modules:/Users/gaomy/Documents/学习/node_modules:/Users/gaomy/Documents/node_modules:/Users/gaomy/node_modules:/Users/node_modules:/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/@antfu+eslint-config-basic@0.19.4_eslint@8.10.0+typescript@4.6.2/node_modules/@antfu/eslint-config-basic/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/@antfu+eslint-config-basic@0.19.4_eslint@8.10.0+typescript@4.6.2/node_modules/@antfu/node_modules:/Users/gaomy/Documents/学习/vue3学习/gmyvue3_project/vue3/node_modules/.pnpm/@antfu+eslint-config-basic@0.19.4_eslint@8.10.0+typescript@4.6.2/node_modules"
